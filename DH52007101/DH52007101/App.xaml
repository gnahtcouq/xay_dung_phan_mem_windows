             xmlns:local="clr-namespace:DH52007101"
             xmlns:localModel="clr-namespace:DH52007101.Models"
             StartupUri="WindowChinh.xaml">
    <Application.Resources>
        <localModel:CMonhoc x:Key="rsMonhoc"/>
        <localModel:CHocvien x:Key="rsHocvien"/>
